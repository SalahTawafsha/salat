<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dp" />
            <stroke android:width="1dip" android:color="#A4587C85" />
            <gradient android:angle="-90" android:startColor="#7C68809A" android:endColor="#7C68809A"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="40dp" />
            <stroke android:width="1dip" android:color="#7C68809A" />
            <solid android:color="#7C68809A"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dp" />
            <stroke android:width="1dip" android:color="#7C68809A" />
            <gradient android:angle="-90" android:startColor="#7C68809A
" android:endColor="#7C00B7FF" />
        </shape>
    </item>
</selector>