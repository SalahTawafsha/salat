<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dp" />
            <stroke android:width="1dip" android:color="#A4855858" />
            <gradient android:angle="-90" android:startColor="#A4855858" android:endColor="#689a92"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="40dp" />
            <stroke android:width="1dip" android:color="#A4855858" />
            <solid android:color="#A4855858"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dp" />
            <stroke android:width="2dip" android:color="#A4855858" />
            <gradient android:angle="-90" android:startColor="#A4A15454" android:endColor="#7AD52626" />
        </shape>
    </item>
</selector>